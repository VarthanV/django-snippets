    "Base File for Django HTML  file with  Materialize CSS": {
        "prefix": "!basemc",
        "body": [
            "{% load static %}",
            "<!DOCTYPE HTML>",
            "<html lang =\"en\">",
            "<head>",
            "<meta charset= \"UTF-8\">",
            "<meta name=\"viewport\" content =\"width = device-width,initial-scale=1.0\">",
            "<meta http-equiv =\"X-UA-Compatible\"  content =\"ie=edge\">",
            "<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css\">",
            "<title> ${1:title} </title>",
            "</head>",
            "<body>",
            "{% block content %}",
            "<h1> Hello World </h1>",
            "",
            "",
            "",
            "{% endblock %}",
            "<script src=\"https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js\"></script>",
            "</body>",
            "<html>"
        ],
        "description": "Base File for Django HTML  file with  Materialize CSS"
    },
    "Extends the Base File": {
        "prefix": "!extend",
        "body": [
            "{% extends '${TM_DIRECTORY}/${TM_FILENAME}' %}",
            "",
            "",
            "{% block content %}",
            "",
            "",
            "{% endblock %}"
        ],
        "description": "Extends the Base File"
    },
    " Simple If Block": {
        "prefix": "!sif",
        "body": [
            "{% if ${1 :condition} %}",
            "$0",
            "{% endif %}"
        ],
        "description": "Simple If Block"
    },
    "If Else Block": {
        "prefix": "!ife",
        "body": [
            "{% if ${1:condition}  %}",
            "",
            "{% else %}",
            "",
            "{% endif %}"
        ],
        "description": "If Else Block"
    },
    "Elif Block": {
        "prefix": "!iff",
        "body": [
            "{% if ${1:condition}  %}",
            "${2:body1}",
            "",
            "{% elif ${3:condition} %}",
            "${4:body2}",
            "",
            "{% elif ${5:condition} %}",
            "${6:body3}",
            "",
            "{% elif  ${7:condition} %}",
            "${8:body3}",
            ""
        ],
        "description": "Elif Block"
    },
    "For loop": {
        "prefix": "!dfor",
        "body": [
            "{% for ${1:item} in ${2:items} %}",
            "${3:body}",
            "",
            "",
            "{% endfor %}"
        ],
        "description": "For loop "
    },
    "Font Awesome CDN ": {
        "prefix": "!fa",
        "body": [
            "<link href=\"https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css]\" rel=\"stylesheet\" integrity=\"sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN\" crossorigin=\"anonymous\">"
        ],
        "description": "Adds Font Awesome CDN"
    },
    "Google Material Icons CDN": {
        "prefix": "!micons",
        "body": [
            "<link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/icon?family=Material+Icons\">"
        ],
        "description": "Adds Google's Material Icon CDN"
    },
    "Sweet Alert CDN": {
        "prefix": "!swal",
        "body": [
            "<script src=\"https://unpkg.com/sweetalert/dist/sweetalert.min.js\"></script>"
        ],
        "description": "Adds Sweet Alert CDN"
    },
    "Link Scripts": {
        "prefix": "!script",
        "body": [
            "<script src=\"{% static '${1:appname}/${2:filename}'%}\"></script> "
        ],
        "description": "Link the Scripts"
    },
    "Link Styles": {
        "prefix": "!css",
        "body": [
            "<link rel=\"stylesheet\" type=\"text/css\" href=\"{% static '${1:appname}/${2:filename}'%}\">"
        ],
        "descriptiion": "Link the CSS"
    },
    "Token": {
        "prefix": "!token",
        "body": [
            "{% csrf_token %}"
        ],
        "description": "Links the CSRF Token "
    }
}